{
  "buildCommand": "nuxt build",
  "installCommand": "yarn install",
  "outputDirectory": ".output/public",
  "devCommand": "nuxt dev",
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
